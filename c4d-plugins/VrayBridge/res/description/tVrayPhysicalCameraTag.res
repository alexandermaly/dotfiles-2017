			LONG VRAYPHYSICALCAMERATAG_BLADESNUM {MIN 0;}
			REAL VRAYPHYSICALCAMERATAG_BLADESROT {MIN 0; MAX 360; STEP 0.1; CUSTOMGUI REALSLIDER;}
			REAL VRAYPHYSICALCAMERATAG_CENTERBIAS {MIN 0; MAX 1; STEP 0.1; CUSTOMGUI REALSLIDER;}
			REAL VRAYPHYSICALCAMERATAG_ANISOTROPY {MIN -1; MAX 1; STEP 0.1; CUSTOMGUI REALSLIDER;}




		}

}
